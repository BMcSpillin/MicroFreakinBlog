<% @users.current do |user| %>
<h3><%= user.handle %></h3>
<h4><%= user.station %></h4>
<p><%= user.bio %></p>
<% end %>

<% @posts.new do |post| %>
  <%= post.timestamp %>
  <%= post.content %>
<% end %>

  
<span><%=post.user_id %></span>
<span><%= post.timestamp %></span>
<p><%= post.content %></p>


    @timestamp = params[:timestamp]
    @content = params[:content]